    <rdfs:comment xml:lang="en">Platforms with buoyancy-based propulsion that are capable of operations at variable depths which are not constrained to be near the sea surface.</rdfs:comment>
    <rdfs:label xml:lang="en">sub-surface gliders</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </glview:PlatformType>
  <glview:PlatformType rdf:about="http://schema.geolink.org/dev/voc/nvs/L06#17">
