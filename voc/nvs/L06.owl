    <rdfs:seeAlso rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/46/"/>
    <rdfs:comment xml:lang="en">An unmanned instrumented platform drifting freely in the water column that periodically makes vertical traverses through the water column (e.g. Argo float).</rdfs:comment>
    <rdfs:label xml:lang="en">drifting subsurface profiling float</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </glview:PlatformType>
