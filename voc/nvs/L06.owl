    <rdfs:subClassOf rdf:resource="http://schema.geolink.org/dev/view#Platform"/>
    <rdfs:seeAlso rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/45/"/>
    <rdfs:comment xml:lang="en">A platform that periodically makes an automated vertical traverse of the water column at a predetermined fixed location. (e.g. YSI vertical profiler, HOMER CTD).</rdfs:comment>
    <rdfs:label xml:lang="en">fixed subsurface vertical profiler</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </glview:PlatformType>
  <glview:PlatformType rdf:about="http://schema.geolink.org/dev/voc/nvs/L06#26">
