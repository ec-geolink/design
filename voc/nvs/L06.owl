    <rdfs:seeAlso rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/60/"/>
    <rdfs:comment xml:lang="en">A platform capable of flight in the atmosphere despite its being heavier than air.</rdfs:comment>
    <rdfs:label xml:lang="en">non-buoyant aircraft</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </glview:PlatformType>
