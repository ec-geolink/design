    <rdfs:seeAlso rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/17/"/>
    <rdfs:comment xml:lang="en">A fixed man-made structure permanently linked to land with access to water at all states of the tide to which instrumentation may be attached (e.g. pier).</rdfs:comment>
    <rdfs:label xml:lang="en">coastal structure</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
