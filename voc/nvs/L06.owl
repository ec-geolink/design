      </owl:Restriction>
    </owl:equivalentClass>
    <rdfs:subClassOf rdf:resource="http://schema.geolink.org/dev/view#Platform"/>
    <rdfs:seeAlso rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/71/"/>
    <rdfs:comment xml:lang="en">A human being without specialised equipment operating on land or the surface of the water column.</rdfs:comment>
    <rdfs:label xml:lang="en">human</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </glview:PlatformType>
