    </owl:equivalentClass>
    <rdfs:subClassOf rdf:resource="http://schema.geolink.org/dev/view#Platform"/>
    <rdfs:seeAlso rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/91/"/>
    <rdfs:comment xml:lang="en">A floating ice sheet detached from the coast.</rdfs:comment>
    <rdfs:label xml:lang="en">ice island</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </glview:PlatformType>
  <glview:PlatformType rdf:about="http://schema.geolink.org/dev/voc/nvs/L06#25">
