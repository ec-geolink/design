    <rdfs:seeAlso rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/6A/"/>
    <rdfs:comment xml:lang="en">A fixed-wing aircraft with no propulsion.</rdfs:comment>
    <rdfs:label xml:lang="en">glider</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </glview:PlatformType>
