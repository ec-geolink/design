    <rdfs:seeAlso rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/37/"/>
    <rdfs:comment xml:lang="en">A small self-propelled platform operating on the surface of the water column in unpredictable locations that is smaller than a ship, but too large to easily remove from the water.</rdfs:comment>
    <rdfs:label xml:lang="en">self-propelled boat</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </glview:PlatformType>
