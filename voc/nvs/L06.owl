    <rdfs:label xml:lang="en">seabird and duck</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </glview:PlatformType>
  <glview:PlatformType rdf:about="http://schema.geolink.org/dev/voc/nvs/L06#45">
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:hasValue rdf:resource="http://schema.geolink.org/dev/voc/nvs/L06#45"/>
