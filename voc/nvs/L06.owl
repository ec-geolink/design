    <rdfs:seeAlso rdf:resource="http://vocab.nerc.ac.uk/collection/L06/current/16/"/>
    <rdfs:comment xml:lang="en">A fixed (for the duration of the measurements) man-made structure away from the coast to which instrumentation may be attached (e.g. oil rig, gas rig or jack-up barge).</rdfs:comment>
    <rdfs:label xml:lang="en">offshore structure</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </glview:PlatformType>
