#################################################################


###  http://schema.geolink.org/1.0/base/main#atPort

glbase:atPort rdf:type owl:ObjectProperty ;
              
              owl:inverseOf glbase:isPortOf ;
              
              rdfs:domain glbase:PortCall ;
              
              rdfs:range glbase:Place ;
              
              rdfs:comment "The domain of this property is PortCall, which acts as the range of 'hasStartPortCall', 'hasMidPortCall', or 'hasEndPortCall' properties." ;
              
              rdfs:label "atPort" .



###  http://schema.geolink.org/1.0/base/main#coReferentOf

glbase:coReferentOf rdf:type owl:ObjectProperty ;
                    
                    owl:inverseOf glbase:coReferentOf ;
                    
                    rdf:type owl:SymmetricProperty ;
                    
                    rdfs:domain owl:Thing ;
                    
                    rdfs:range owl:Thing ;
                    
                    rdfs:comment "This property is used to state that an object (instance) matches with another object. This can either be a result of manual matching or running some coreference resolution algorithm." ;
                    
                    rdfs:label "coReferentOf" .



