<http://schema.geolink.org/1.0/base/main> rdf:type owl:Ontology ;
                                          
                                          owl:imports <http://schema.geolink.org/1.0/voc/nvs/L06> ,
                                                      <http://schema.geolink.org/1.0/voc/nvs/P02> ,
                                                      <http://schema.geolink.org/1.0/voc/nvs/P03> ,
                                                      <http://schema.geolink.org/1.0/base/physicalsample> ,
                                                      <http://schema.geolink.org/1.0/voc/nvs/C17> ,
                                                      <http://schema.geolink.org/1.0/voc/nvs/L22> ,
                                                      <http://schema.geolink.org/1.0/voc/gebco/featuretype> ,
                                                      <http://schema.geolink.org/1.0/voc/local> ,
                                                      <http://schema.geolink.org/1.0/voc/nvs/L05> ;
                                          
                                          rdfs:label "GeoLink base ontology." ;
                                          
                                          rdfs:comment "A base ontology providing a conceptual view into the GeoLink patterns." .


#################################################################
#
#    Datatypes
#
#################################################################


###  http://www.opengis.net/ont/geosparql#wktLiteral

geosparql:wktLiteral rdf:type rdfs:Datatype ;
                     
                     rdfs:comment "Enables the use of Well Known Text strings" ;
                     
                     rdfs:label "geosparql:wktLiteral" .



###  http://www.w3.org/1999/02/22-rdf-syntax-ns#HTML

rdf:HTML rdf:type rdfs:Datatype ;
         
         rdfs:comment "Enables the use of HTML strings" ;
         
         rdfs:label "rdf:HTML" .



###  http://www.w3.org/2001/XMLSchema#gYear

xsd:gYear rdf:type rdfs:Datatype .



###  http://www.w3.org/2001/XMLSchema#gYearMonth

xsd:gYearMonth rdf:type rdfs:Datatype .





