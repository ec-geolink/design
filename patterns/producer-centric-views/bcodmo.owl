    <owl:DatatypeProperty rdf:about="&bcodmo;acquisitionDescription">
        <rdfs:comment>Maps to full patterns ???</rdfs:comment>
        <rdfs:domain rdf:resource="&ecglview;Dataset"/>
        <rdfs:range rdf:resource="&rdf;HTML"/>
    </owl:DatatypeProperty>
    
    <!-- http://schema.geolink.org/dev/bcoview#processingDescription -->
    
    <owl:DatatypeProperty rdf:about="&bcodmo;processingDescription">
        <rdfs:comment>Maps to full patterns ???</rdfs:comment>
        <rdfs:domain rdf:resource="&ecglview;Dataset"/>
        <rdfs:range rdf:resource="&rdf;HTML"/>
    </owl:DatatypeProperty>
    
    <!-- http://schema.geolink.org/dev/bcoview#givenName -->
    
    <owl:DatatypeProperty rdf:about="&bcodmo;givenName">
        <rdfs:comment>Maps to full patterns datatype property PersonName:firstOrGivenName</rdfs:comment>
        <rdfs:domain rdf:resource="&ecglview;Person"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <!-- http://schema.geolink.org/dev/bcoview#surname -->
    
    <owl:DatatypeProperty rdf:about="&bcodmo;surname">
        <rdfs:comment>Maps to full patterns datatype property PersonName:familyOrSurname</rdfs:comment>
        <rdfs:domain rdf:resource="&ecglview;Person"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <!-- http://schema.geolink.org/dev/bcoview#startDate -->
    
    <owl:DatatypeProperty rdf:about="&bcodmo;startDate">
        <rdfs:comment>Maps to full patterns ???</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ecglview;Cruise"/>
                    <rdf:Description rdf:about="&ecglview;Program"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    
    <!-- http://schema.geolink.org/dev/bcoview#endDate -->
    
    <owl:DatatypeProperty rdf:about="&bcodmo;endDate">
        <rdfs:comment>Maps to full patterns datatype property ???</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ecglview;Cruise"/>
                    <rdf:Description rdf:about="&ecglview;Program"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    
    <!-- http://schema.geolink.org/dev/bcoview#mediaType -->

    <owl:DatatypeProperty rdf:about="&bcodmo;mediaType">
        <rdfs:domain rdf:resource="&ecglview;DigitalObject"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
