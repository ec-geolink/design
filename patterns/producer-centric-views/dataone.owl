<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#">
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
    <!ENTITY geosparql "http://www.opengis.net/ont/geosparql#">
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <!ENTITY ecglview "http://schema.geolink.org/dev/view#">
    <!ENTITY dataone "http://schema.geolink.org/dev/doview#">
]>

<rdf:RDF xmlns="http://schema.geolink.org/dev/doview#"
     xml:base="http://schema.geolink.org/dev/doview"
     xmlns:geosparql="http://www.opengis.net/ont/geosparql#"
     xmlns:dataone="http://schema.geolink.org/dev/doview#"
     xmlns:ecglview="http://schema.geolink.org/dev/view#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <owl:Ontology rdf:about="http://schema.geolink.org/dev/doview"/>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://schema.geolink.org/dev/doview#hasSubmitter -->

    <owl:DatatypeProperty rdf:about="&dataone;hasSubmitter">
        <rdfs:domain rdf:resource="&ecglview;DigitalObject"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ecglview;Person"/>
                    <rdf:Description rdf:about="&ecglview;Organization"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:DatatypeProperty>


    <!-- http://schema.geolink.org/dev/doview#hasRightsHolder -->

    <owl:DatatypeProperty rdf:about="&dataone;hasRightsHolder">
        <rdfs:domain rdf:resource="&ecglview;DigitalObject"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ecglview;Person"/>
                    <rdf:Description rdf:about="&ecglview;Organization"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:DatatypeProperty>


    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

     <!-- http://schema.geolink.org/dev/doview#hasChecksumAlgorithm -->

     <owl:DatatypeProperty rdf:about="&dataone;hasChecksumAlgorithm">
         <rdfs:domain rdf:resource="&ecglview;DigitalObject"/>
         <rdfs:range rdf:resource="&xsd;string"/>
     </owl:DatatypeProperty>


     <!-- http://schema.geolink.org/dev/doview#dateUploaded -->

     <owl:DatatypeProperty rdf:about="&dataone;dateUploaded">
         <rdfs:domain rdf:resource="&ecglview;DigitalObject"/>
         <rdfs:range rdf:resource="&xsd;dateTime"/>
     </owl:DatatypeProperty>


    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


</rdf:RDF>
