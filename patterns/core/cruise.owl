                <owl:inverseOf rdf:resource="http://schema.geolink.org/dev/pattern/cruise#hasSpatialFootprint"/>
            </rdf:Description>
            <rdf:Description rdf:about="http://schema.geolink.org/dev/pattern/cruise#rollifiedPort"/>
