     -->

    


    <!-- http://schema.geolink.org/1.0/pattern/person#hasPersonalInfoItem -->

    <owl:ObjectProperty rdf:about="http://schema.geolink.org/1.0/pattern/person#hasPersonalInfoItem">
        <rdfs:label>hasPersonalInfoItem</rdfs:label>
    </owl:ObjectProperty>
    


